{
  "name": "@nqframework/data-fetcher",
  "version": "1.2.2",
  "description": "A data fetcher component for the server and the connector client",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "jsnext:main": "build/index.es.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "dev": "ts-node-dev ./src/index.ts",
    "lint": "",
    "format": "npm run lint",
    "release": "standard-version",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "build",
    "LICENCE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nq-framework/nq-data-fetcher.git"
  },
  "keywords": [],
  "author": "Milos Spasojevic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nq-framework/nq-data-fetcher/issues"
  },
  "homepage": "https://github.com/nq-framework/nq-data-fetcher#readme",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "rollup": "^2.33.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.29.0",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@nqframework/models": "^4.0.2",
    "@types/tedious": "^4.0.2",
    "tedious": "^9.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
