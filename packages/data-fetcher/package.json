{
  "name": "@nqframework/data-fetcher",
  "version": "1.7.1",
  "description": "A data fetcher component for the server and the connector client",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "jsnext:main": "build/index.es.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start:dev": "tsc --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
  },
  "files": [
    "build",
    "LICENCE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nq-framework/nq-data-fetcher.git"
  },
  "keywords": [],
  "author": "Milos Spasojevic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nq-framework/nq-data-fetcher/issues"
  },
  "homepage": "https://github.com/nq-framework/nq-data-fetcher#readme",
  "devDependencies": {
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@nqframework/models": "^4.4.2",
    "@types/tedious": "^4.0.2",
    "npm-check-updates": "^10.2.2",
    "tedious": "^9.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "gitHead": "965c7307afc43ee34597ff88ebdbfc2965c83645"
}
